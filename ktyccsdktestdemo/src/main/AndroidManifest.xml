<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.xxxx.tky">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:name="com.xxxx.tky.MyApplicaiton"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="android:name,android:icon">
        <activity
            android:name="com.xxxx.tky.activity.LoginActivity"
            android:theme="@style/LaunchTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.xxxx.tky.activity.CallHistoryFragmentActivity" />
        <activity android:name="com.xxxx.tky.activity.HomeActivity" />
        <activity android:name="com.xxxx.tky.activity.GuideActivity" />
        <activity android:name="com.xxxx.tky.activity.ContactDetailActivity" />
        <activity android:name="com.xxxx.tky.activity.CustomPersonActivity" />
        <activity android:name="com.xxxx.tky.activity.SearchPersonActivity" />
        <activity android:name="com.xxxx.tky.activity.CustomPesonDetailActivity" />
        <activity
            android:name="com.xxxx.tky.activity.CustomPersonDetailToDetailActivity"
            android:windowSoftInputMode="adjustPan|stateHidden" />
        <activity android:name="com.xxxx.tky.activity.MineCustomPersonActivity" />

    </application>

</manifest>